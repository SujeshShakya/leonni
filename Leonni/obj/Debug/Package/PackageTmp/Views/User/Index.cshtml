@model UserIndexModel
@{
    ViewBag.Title = "Index";    
    var commentUrl = ViewData["Url"] ?? "";
    var userID =  "f22511dc-37d7-456d-9da7-2b9dbbe316e0";//((MembershipUser)Session["CurrentUser"]).ProviderUserKey.ToString();
    var profileId = ViewData["ProfileId"]; 
}
<div id="content-wrapper">
    <div class="middle-hr">
    </div>
    <div id="banner">
        <img src="@Url.Action("Picture", "File", new { id = Model.AdvertisingBanner, width = 720, height = 234 })" alt="" />
    </div>
    <div id="youtube-container">
        @*<img src="img/youtube.jpg" alt="" />*@
        <iframe width="720" height="450" src="@Model.VideoUrl" frameborder="0" allowfullscreen>
        </iframe>
    </div>
    <div id="image-showcase">
        @*<img src="img/image-gallery.jpg" alt="" />*@
        @if (Model.BannerList != null)
        {
            foreach (var objModel in Model.BannerList)
            { 
            <img src="@Url.Action("Picture", "File", new { id = objModel.FileId, width = 720, height = 234 })" alt="" /><br />
            }
        }
    </div>
    @*<div id="banner-groups" style="background: #e0b3ee; width:100%; height: 155px;
        margin-bottom: 20px;">
    </div>*@
    <div class="middle-hr">
    @*@Html.Partial("Comments", new ViewDataDictionary { { "Url", Request.RawUrl.ToString() }, { "ProfileId", profileId } })*@
    </div>
    @{Html.RenderAction("UserSearch", "User");}

    @section RightMenu
    {
        @{Html.RenderPartial("NormalRightMenu");}      
    }
    
